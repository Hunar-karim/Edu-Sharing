{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-edusharing": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "baseHref": "/edu-sharing/",
            "deployUrl": "/edu-sharing/",
            "assets": [
              { "glob": "**/*", "input": "src/assets/", "output": "/assets/" },
              { "glob": "**/*", "input": "node_modules/ngx-monaco-editor/assets/monaco", "output": "/assets/monaco/" },
              { "glob": "**/*", "input": "node_modules/tinymce", "output": "/tinymce/" },
              { "glob": "**/*", "input": "node_modules/tinymce-i18n/langs5", "output": "/tinymce/langs/" }
            ],
            "styles": [
              "src/app/core-ui-module/styles/core-base.scss",
              "src/styles/main.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "src/assets/js/nouislider.min.js",
              "node_modules/materialize-css/dist/js/materialize.min.js",
              "node_modules/moment/min/moment-with-locales.min.js",
              "node_modules/chart.js/dist/Chart.js",
              "node_modules/tinymce/tinymce.min.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "15kb",
                  "maximumError": "100kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-edusharing:build",
            "proxyConfig": "src/proxy.conf.js"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-edusharing:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-edusharing:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "./karma.conf.js",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "src/assets/js/nouislider.min.js",
              "node_modules/materialize-css/dist/js/materialize.min.js",
              "node_modules/moment/min/moment-with-locales.min.js",
              "node_modules/chart.js/dist/Chart.js"
            ],
            "styles": [
              "src/styles/main.scss"
            ],
            "assets": [
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "angular-edusharing-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "angular-edusharing:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    },
    "edu-sharing-api": {
      "projectType": "library",
      "root": "projects/edu-sharing-api",
      "sourceRoot": "projects/edu-sharing-api/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/edu-sharing-api/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/edu-sharing-api/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/edu-sharing-api/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/edu-sharing-api/src/test.ts",
            "tsConfig": "projects/edu-sharing-api/tsconfig.spec.json",
            "karmaConfig": "projects/edu-sharing-api/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "angular-edusharing",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "es",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "es"
    }
  }
}
